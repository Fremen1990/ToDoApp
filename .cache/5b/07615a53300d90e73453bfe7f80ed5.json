generated":{"line":72,"column":56},"source":"src/components/TaskList.js","original":{"line":52,"column":27}},{"generated":{"line":72,"column":61},"source":"src/components/TaskList.js","original":{"line":52,"column":17}},{"generated":{"line":72,"column":62},"source":"src/components/TaskList.js","original":{"line":52,"column":33}},{"generated":{"line":72,"column":63},"source":"src/components/TaskList.js","original":{"line":52,"column":17}},{"generated":{"line":72,"column":65},"source":"src/components/TaskList.js","original":{"line":52,"column":36}},{"generated":{"line":72,"column":66},"source":"src/components/TaskList.js","original":{"line":52,"column":17}},{"generated":{"line":72,"column":67},"source":"src/components/TaskList.js","original":{"line":49,"column":12}},{"generated":{"line":72,"column":68},"source":"src/components/TaskList.js","original":{"line":43,"column":8}},{"generated":{"line":72,"column":69},"source":"src/components/TaskList.js","original":{"line":42,"column":4}},{"generated":{"line":73,"column":0},"source":"src/components/TaskList.js","original":{"line":56,"column":1}},{"generated":{"line":73,"column":1},"source":"src/components/TaskList.js","original":{"line":4,"column":0}},{"name":"TaskList","generated":{"line":75,"column":15},"source":"src/components/TaskList.js","original":{"line":58,"column":15}}],"sources":{"src/components/TaskList.js":"import React from 'react';\nimport Task from './Task';\n\nconst TaskList = (props) => {\n\n    const active = props.tasks.filter(task =>\n        task.active)\n\n    const done = props.tasks.filter(task => !task.active)\n\n\n    if (done.length >= 2) {\n        done.sort((a, b) => a.finishDate - b.finishDate) // alternative sort method  - short version\n    }\n\n    // done.sort((a, b) => {  // alternative sort method full version\n    //     if (a.finishDate > b.finishDate) {\n    //         return -1\n    //     }\n    //     if (a.finishDate < b.finishDate) {\n    //         return 1\n    //     }\n    //     return 0\n    // })\n\n    if (active.length >= 2) {\n        active.sort((a, b) => {\n            a = a.text.toLowerCase();\n            b = b.text.toLowerCase();\n            if (a < b) return -1;\n            if (a > b) return 1;\n            return 0;\n        })\n\n    }\n    // console.log(active, done)\n\n    const activeTasks = active.map(task => <Task key={task.id} task={task} delete={props.delete} change={props.change} />)\n\n    const doneTasks = done.map(task => <Task key={task.id} task={task} delete={props.delete} change={props.change} />)\n\n    return (\n        <>\n            <div className=\"active\">\n                <h2>Task List</h2>\n                {activeTasks.length > 0 ? activeTasks : <p>No tasks to do!</p>}\n            </div>\n            <hr />\n            <div className=\"done\">\n                <h3>Done <em>({done.length})</em></h3>\n                {done.length > 3 && <span style={{ fontSize: 16 }}>There are only 3 last tasks diplayed</span>}\n                {doneTasks.slice(0, 3)}\n            </div>\n        </>\n    )\n}\n\nexport default TaskList;"},"lineCount":null}},"error":null,"hash":"4269a592436d0ef19992f53df59d8c15","cacheData":{"env":{}}}