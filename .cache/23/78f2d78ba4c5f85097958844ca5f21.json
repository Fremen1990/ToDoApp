nerated":{"line":120,"column":23},"source":"src/components/AddTask.js","original":{"line":50,"column":16}},{"generated":{"line":121,"column":0},"source":"src/components/AddTask.js","original":{"line":50,"column":24}},{"generated":{"line":121,"column":8},"source":"src/components/AddTask.js","original":{"line":50,"column":24}},{"generated":{"line":121,"column":15},"source":"src/components/AddTask.js","original":{"line":50,"column":31}},{"generated":{"line":121,"column":17},"source":"src/components/AddTask.js","original":{"line":50,"column":33}},{"name":"handleClick","generated":{"line":121,"column":22},"source":"src/components/AddTask.js","original":{"line":50,"column":38}},{"generated":{"line":122,"column":0},"source":"src/components/AddTask.js","original":{"line":50,"column":16}},{"generated":{"line":122,"column":15},"source":"src/components/AddTask.js","original":{"line":43,"column":12}},{"generated":{"line":122,"column":16},"source":"src/components/AddTask.js","original":{"line":42,"column":8}},{"generated":{"line":123,"column":0},"source":"src/components/AddTask.js","original":{"line":53,"column":5}},{"name":"Component","generated":{"line":127,"column":2},"source":"src/components/AddTask.js","original":{"line":4,"column":22}},{"name":"AddTask","generated":{"line":129,"column":15},"source":"src/components/AddTask.js","original":{"line":56,"column":15}}],"sources":{"src/components/AddTask.js":"import React, { Component } from 'react'\nimport './AddTask.css';\n\nclass AddTask extends Component {\n\n    minDate = new Date().toISOString().slice(0, 10)\n\n    state = {\n        text: '',\n        checked: false,\n        date: this.minDate,\n    }\n\n    handleText = (e) => {\n        this.setState({\n            text: e.target.value\n        })\n    }\n\n    handleCheckBox = (e) => {\n        this.setState({\n            checked: e.target.checked\n        })\n    }\n\n    handleDate = (e) => {\n        this.setState({\n            date: e.target.value\n        })\n    }\n\n    handleClick = () => {\n        console.log(\"add\")\n    }\n\n    render() {\n        let maxDate = this.minDate.slice(0, 4) * 1 + 1// multiply x1 to convert to number +1 year\n        console.log(maxDate)\n        maxDate = maxDate + \"-12-31\" // final 2021-12-31\n        console.log(maxDate)\n\n        return (\n            <div className=\"form\">\n                <input type=\"text\" placeholder=\"add task\" value={this.state.text} onChange={this.handleText} />\n                <input type=\"checkbox\" checked={this.state.checked} id=\"important\" onChange={this.handleCheckBox} />\n                <label htmlFor=\"important\">IMPORTANT </label>\n                <br />\n                <label htmlFor=\"date\">Deadline</label>\n                <input type=\"date\" value={this.state.date} min={this.minDate} max={maxDate} onChange={this.handleDate} />\n                <button onClick={this.handleClick}>Add</button>\n            </div>\n        );\n    }\n}\n\nexport default AddTask;"},"lineCount":null}},"error":null,"hash":"c6e3f238c16d97bf8ccddfc3fd304bba","cacheData":{"env":{}}}